{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "app-old#dev": {
      "dependsOn": ["schema#build", "//#generate"]
    },
    "email-worker#dev": {
      "dependsOn": ["schema#build", "//#generate"]
    },
    "schema#build:dev": {
      "cache": false
    },
    "//#generate": {
      "cache": false
    },

    "build": {
      "cache": false
    },
    "app#build": {
      "dependsOn": ["schema#build", "//#generate"]
    },
    "email-worker#build": {
      "dependsOn": ["schema#build", "//#generate"]
    },
    "order-cancel-worker#build": {
      "dependsOn": ["schema#build", "//#generate"]
    },
    "start": {
      "cache": false
    }
  }
}
