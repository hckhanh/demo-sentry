{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "schema#dev": {
      "persistent": false
    },
    "app#dev": {
      "dependsOn": ["schema#dev"]
    },
    "db:dev": {
      "cache": false
    },
    "generate": {
      "cache": false
    },
    "init": {
      "cache": false
    },
    "destroy": {
      "cache": false
    }
  }
}
