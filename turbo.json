{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "app#dev": {
      "dependsOn": ["schema#build:dev", "schema#generate"]
    },
    "schema#build:dev": {
      "cache": false
    },
    "schema#generate": {
      "cache": false
    },
    "schema#migrate": {
      "cache": false
    },
    "schema#seed": {
      "cache": false
    }
  }
}
